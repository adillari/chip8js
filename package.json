{
  "devDependencies": {
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.4"
  },
  "scripts": {
    "build": "npx tsc",
    "build:css": "npx tailwindcss -i ./tailwind.css -o ./public/application.css",
    "lint": "npx prettier ./*.* ./public/js/*.js ./src/*.* --write --log-level debug",
    "lint:check": "npx prettier ./*.* ./public/js/*.js ./src/*.* --check --log-level debug",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "typescript": "^5.5.2"
  }
}
